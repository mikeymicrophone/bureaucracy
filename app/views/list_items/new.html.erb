<h1>New list_item</h1>

<% form_for(@list_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :list_id %><br />
    <%= f.text_field :list_id %>
  </p>
  <p>
    <%= f.label :belonging_id %><br />
    <%= f.text_field :belonging_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', list_items_path %>